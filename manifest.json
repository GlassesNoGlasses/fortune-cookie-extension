{
  "name": "Fortune Cookie!",
  "description": "Random daily fortunes anywhere, anytime.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "/images/cookie_48.png",
      "128": "/images/cookie_128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "48": "/images/cookie_48.png",
    "128": "/images/cookie_128.png"
  }
}
